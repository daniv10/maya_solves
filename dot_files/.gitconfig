# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
        name = Maya Gershovitz Bar
        email = mgershovitz@gmail.com
[alias]
        co = checkout
        br = branch
        ci = commit
        st = status
        up = pull --rebase
        sq = "!f() { git rebase -i HEAD~$1; }; f"
        fix = "!f() { git add .; git commit -m "[fixup]"; }; f"
        fixit = "!f() { git add .; git commit --amend; }; f"
        open = "!f() { url="$(git config --get remote.origin.url | tr : /)"; full_url="$(echo "${url/git@/http://}")"; open $full_url; }; f"
        discard = reset --hard
        cbr = "!gitCurrentBranchName() { branchName="$(git name-rev $1)"; echo ${branchName#"${1} "}; }; gitCurrentBranchName"
[core]
        excludesfile = /Users/mayagershovitzbar/.gitignore_global
[filter "lfs"]
        smudge = git-lfs smudge -- %f
        process = git-lfs filter-process
        required = true
        clean = git-lfs clean -- %f
[init]
        templatedir = /Users/mayagershovitzbar/.git_template
